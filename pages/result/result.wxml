<!--pages/result/result.wxml-->
<view class="title"> 战绩 </view>
<view class="container">
    <scroll-view class="result_scroll" scroll-y>
        <view class="result_list" wx:for="{{resultList}}">
            <view class="result_item"> 
                <image class= 'result_image' src='/images/level0.png'> </image>
                <view class= 'result_type' > {{index}}: {{resultList.item_type}} </view>
                <view class= 'result_time'> {{index}}: {{resultList.item_time}} </view>
                <view class= 'result_date'> {{index}}: {{resultList.item_date}} </view>
            </view>
        </view>
    </scroll-view>
</view>