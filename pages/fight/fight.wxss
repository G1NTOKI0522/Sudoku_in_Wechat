/* pages/fight/fight.wxss */
.userinfo {  
  display: flex;  
  flex-direction: column;  
  align-items: center;  
}  
  
.userinfo-avatar {  
  width: 128rpx;  
  height: 128rpx;  
  margin: 20rpx;  
  border-radius: 50%;  
}  
  
.userinfo-nickname {  
  color: #aaa;  
}  
  
.usermotto {  
  margin-top: 200px;  
} 

.cell{
  border:3px solid black;
  border-radius: 24rpx;
}

#create{
  position: absolute;
  top: 350rpx;
  left: 160rpx;
  width: 400rpx;
  height: 150rpx;
  line-height: 125rpx;
  font-size: 70rpx;
  text-align: center;
}

#enter{
  position: absolute;
  top: 600rpx;
  left: 160rpx;
  width: 400rpx;
  height: 150rpx;
  line-height: 125rpx;
  font-size: 70rpx;
  text-align: center;
}